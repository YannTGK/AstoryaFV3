{
  "expo": {
    "name": "AstoryaFV3",
    "slug": "astorya-fv3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to your microphone for recording and playback."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.AstoryaFV3",
      "permissions": [
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bfff67fe-5008-44d8-a095-3ebb8bada173",
      "fallbackToCacheTimeout": 0
    },
    "owner": "dyangoo",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bfff67fe-5008-44d8-a095-3ebb8bada173"
      }
    }
  }
}
